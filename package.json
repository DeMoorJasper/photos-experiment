    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "classnames": "^2.2.6",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-babel-typescript": "^1.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "swr": "^0.1.15",
    "tailwindcss": "^1.1.4",
    "wouter": "^2.4.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.0.5",
